      "email": "oopayne42@students.tntech.edu"
    }
  ],
  "version": "0.0.0",
  "main": "server.js",
  "private": true,
  "dependencies": {
    "body-parser": "~1.14.2",
    "bootstrap": "~3.3.6",
    "clientconfig": "~1.1",
    "cookie-parser": "~1.4.1",
    "domready": "~1.0.8",
    "favicon-setter": "^0.1.1",
    "feathers": "~1.3.0",
    "feathers-client": "~0.5.1",
    "feathers-sequelize": "~1.1.1",
    "feathers-socketio": "~1.2.0",
    "getconfig": "~2.2",
    "helmet": "~1.0",
    "lodash.isequal": "^4.0.0",
    "moonboots-express": "~3.0",
    "mysql": "~2.10.2",
    "react": "~0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "~0.14.7",
    "react-router": "~2.0.0",
    "sequelize": "~3.19.1",
    "sequelize-cli": "~2.3.1",
    "serve-static": "^1.10.2",
    "socket.io": "~1.4.5 ",
    "socket.io-client": "~1.4.5",
    "stylizer": "~2.1",
    "validator": "~4.5"
  },
  "peerDependencies": {},
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.0.0",
    "eslint-plugin-react": "^3.16.1",
    "fsmonitor": "^0.2.4",
    "js-beautify": "~1",
    "precommit-hook": "~3",
    "shelljs": "^0.6.0"
  },
  "eslingConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "curly": [
        2,
        "all"
      ],
      "no-console": 0,
      "comma-style": [
        2,
        "last"
      ],
      "indent": [
        2,
        2
      ],
      "semi": [
        2,
        "always"
      ],
      "quotes": [
        2,
        "single"
      ],
      "no-undef": 2,
      "no-unused-vars": 2,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1
    },
    "globals": {
      "window": true,
      "document": true
    },
    "plugins": [
      "react"
    ]
  },
  "scripts": {
    "build": "rm -rf client/ && babel --presets es2015,react -d client/ src/",
    "start": "node server.js",
    "dev": "NODE_ENV=dev node server",
    "prod": "NODE_ENV=prod node server",
    "lint": "eslint .",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}
